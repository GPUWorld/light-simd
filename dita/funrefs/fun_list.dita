            </row>
            <row>
              <entry><xref href="sqrt.dita">sqrt</xref></entry>
              <entry>square root</entry>
            </row>
            <row>
              <entry>rcp</entry>
              <entry>reciprocal</entry>
            </row>
            <row>
              <entry>rsqrt</entry>
              <entry>reciprocal of square root</entry>
            </row>
            <row>
              <entry>hypot</entry>
              <entry>hypotenuse</entry>
            </row>
            <row>
              <entry>cube</entry>
              <entry>cube (raised to power 3)</entry>
            </row>
            <row>
              <entry>cbrt</entry>
              <entry>cubic root</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </section>
    <section>
