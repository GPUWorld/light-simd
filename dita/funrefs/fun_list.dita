  <body>
    <section>
      <title>Rounding functions</title>
      <table frame="topbot" id="table_round_funs">
        <tgroup cols="2">
          <colspec colname="name" colnum="1" colwidth="100pt"/>
          <colspec colname="desc" colnum="2" colwidth="320pt"/>
          <tbody>
            <row>
              <entry>floor</entry>
              <entry>nearest integer lower bound</entry>
            </row>
            <row>
              <entry>ceil</entry>
              <entry>nearest integer upper bound</entry>
            </row>
            <row>
              <entry>trunc</entry>
              <entry>rounding by truncation</entry>
            </row>
            <row>
              <entry>round</entry>
              <entry>rounding to nearest integer</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </section>
